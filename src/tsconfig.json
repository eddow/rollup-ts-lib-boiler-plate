{
	"compilerOptions": {
		"module": "es2022",
		"target": "ESNext",
		"sourceMap": true,
		"declaration": false,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true,
		"strictBindCallApply": false,
		"baseUrl": "."
	},
	"include": ["./**/*.ts"]
}
